  <revhistory>
    <revision>
      <revnumber>&version;</revnumber>
      <authorinitials>enz</authorinitials>
      <revremark>
	<itemizedlist>
	  <listitem>
	    Fix cursor not positioned at last moves if handicap or setup
	    stones exist.
	  </listitem>
	  <listitem>
	    Fix bug: handicap stones were played after setup done.
	  </listitem>
	  <listitem>
	    Clear old markups before showing an analyze command with
	    a point list result.
	  </listitem>
	  <listitem>
	    Store komi in preferences.
	  </listitem>
	  <listitem>
	    Add command reference to documentation.
	  </listitem>
	  <listitem>
	    Other small fixes.
	  </listitem>
	</itemizedlist>
      </revremark>
    </revision>
